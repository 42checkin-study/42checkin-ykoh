{
  "name": "42checkin-ykoh",
  "version": "1.0.0",
  "description": "simple checkin app using express, ts, sequelize and handlebars.",
  "main": "server.ts",
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "npm run test -- --watchAll",
    "start": "ts-node ./src/server.ts",
    "start:dev": "nodemon ./src/server.ts",
    "start:prod": "NODE_ENV=production node -r ts-node/register/transpile-only ./src/server.ts",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/42checkin-study/42checkin-ykoh.git"
  },
  "author": "ykoh42",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/42checkin-study/42checkin-ykoh/issues"
  },
  "homepage": "https://github.com/42checkin-study/42checkin-ykoh#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.9",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.11.0",
    "sequelize-typescript": "^2.1.1",
    "winston": "^3.3.3"
  }
}
